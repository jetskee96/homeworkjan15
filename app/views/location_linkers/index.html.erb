<h1>Listing location_linkers</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_linkers.each do |location_linker| %>
      <tr>
        <td><%= location_linker.location %></td>
        <td><%= location_linker.description %></td>
        <td><%= link_to 'Show', location_linker %></td>
        <td><%= link_to 'Edit', edit_location_linker_path(location_linker) %></td>
        <td><%= link_to 'Destroy', location_linker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location linker', new_location_linker_path %>
